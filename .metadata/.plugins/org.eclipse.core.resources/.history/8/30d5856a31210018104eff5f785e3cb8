	public static int addIntegers(int firstNumber , int secondNumber) {
		if(firstNumber == 0 || secondNumber == 0) {
			int result = firstNumber + secondNumber;
			return result;
		} else {
		
			int result = firstNumber + secondNumber;
			return result;
		}
//		 else {
//			int result = firstNumber * secondNumber;
//			return result;
//		}
	
	}


