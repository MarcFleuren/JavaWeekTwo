
public class HelloWorld {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] resultsArray = new int[15][15];
			for (int i = 0 ; i <= 10 ; i++) {
				for(int y = 0; y <= 10; y++) {
					resultsArray[i][y] = addIntegers(i,y);
					System.out.print(resultsArray[i][y]);
					
				}
				System.out.println(resultsArray[i][i]*10);
			}
			
		
	}
