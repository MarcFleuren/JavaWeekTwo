
public class UniqueSum1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(uniqueAddition(3,3,3));
	}
	public static int uniqueAddition(int a, int b, int c) {
		int total = 0;
		if (!(a == b || a == c)) {
			total = a;
				}
		if (!(b == c || b == a)) {
			total = total + b;
		}
		if (!(b == c || c == a)) {
			total = total + c;
		}
			
		return total;
	}		
}
