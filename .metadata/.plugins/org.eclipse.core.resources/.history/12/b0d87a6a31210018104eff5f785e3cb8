
public class TooHot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(tooHot(75,false));

	}
	public static Boolean tooHot(int temp, boolean isSummer) {
		if(isSummer == true &&  temp <= 100 && temp >= 60) {
			boolean result = true;
			return result;
			
		}
		if(isSummer == false && temp <= 90 && temp>= 60) {
			boolean result = true;
			return result;
		
		} else {
			boolean result = false;
			return result;
		}
	}
}
